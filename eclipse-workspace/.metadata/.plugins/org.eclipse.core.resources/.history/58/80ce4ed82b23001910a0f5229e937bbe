package random;

import java.util.*;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;

public class Difff 
{
	private int lines = 0;
	private ArrayList<String> words = new ArrayList<>();
	private ArrayList<String> commonWords = new ArrayList<>(2);
	private String common = "";
	private String word1 = "";
	private String word2 = "";
	private String c = "";

	public static void main(String[] args) 
	{
		Difff d = new Difff();
		d.diff();
	}

	public void diff()
	{
		Scanner scan = new Scanner(System.in);
		
		while(lines < 1)
		{
			word1 = scan.next();
			word2 = scan.next();
			words.add(word1);
			words.add(word2);
			lines++;
		}			
		
		scan.close();
		System.out.print(words);
		
		for(int i = 0; i < words.size(); i+=2)
		{
			word1 = words.get(i);
			word2 = words.get(i+1);
			
			String w1 = word1;
			String w2 = word2;
			while(w1.length() > 0)
			{
				c = w1.charAt(0)+"";
				if(w2.contains(c))
				{
					common += c;
					w2 = w2.substring(w2.indexOf(c), w2.length());
				}
				w1 = w1.substring(1, w1.length());
			}
			commonWords.add(common);
			
			common = "";
			w1 = word1;
			w2 = word2;
			while(w2.length() > 0)
			{
				c = w2.charAt(0)+"";
				if(w1.contains(c))
				{
					common += c;
					w1 = w1.substring(w1.indexOf(c), w1.length());
				}
				w2 = w2.substring(1, w2.length());
			}
			commonWords.add(common);
			
			common = "";
			w1 = word1;
			w2 = word2;
			while(w1.length() > 0)
			{
				c = w1.charAt(w1.length()-1)+"";
				if(w2.contains(c))
				{
					common += c;
					w2 = w2.substring(0, w2.lastIndexOf(c));
				}
				w1 = w1.substring(0, w1.lastIndexOf(c));
			}
			commonWords.add(common);
			
			common = "";
			w1 = word1;
			w2 = word2;
			while(w2.length() > 0)
			{
				c = w2.charAt(w2.length()-1)+"";
				if(w1.contains(c))
				{
					common += c;
					w1 = w1.substring(0, w1.lastIndexOf(c));
				}
				w2 = w2.substring(0, w2.lastIndexOf(c));
			}
			char[] sort = common.toCharArray();
			Arrays.sort(sort);
			common = sort.toString();
			commonWords.add(common);
			System.out.println(commonWords);
		}
		
	}
		
}

